<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_market_home"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.myapplication.ui.page.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/mineFragment"
        android:name="com.example.myapplication.ui.page.mine.MineFragment"
        android:label="MineFragment" />
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.myapplication.ui.page.detail.DetailFragment"
        android:label="DetailFragment" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.myapplication.ui.page.search.SearchFragment"
        android:label="SearchFragment" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.myapplication.ui.page.setting.SettingFragment"
        android:label="SettingFragment" />
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="com.example.myapplication.ui.page.feedback.FeedbackFragment"
        android:label="FeedbackFragment" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.myapplication.ui.page.login.LoginFragment"
        android:label="LoginFragment" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.myapplication.ui.page.register.RegisterFragment"
        android:label="RegisterFragment" />
    <fragment
        android:id="@+id/appDetailFragment"
        android:name="com.example.myapplication.ui.page.detail.AppDetailFragment"
        android:label="AppDetailFragment">
        <argument
            android:name="appId"
            app:argType="string"
            app:nullable="false" />
    </fragment>



    <action
        android:id="@+id/goHomeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/goMineFragment"
        app:destination="@id/mineFragment" />
    <action
        android:id="@+id/goDetailFragment"
        app:destination="@id/detailFragment" />
    <action
        android:id="@+id/goSearchFragment"
        app:destination="@id/searchFragment" />
    <action
        android:id="@+id/goAppDetailFragment"
        app:destination="@id/appDetailFragment" />
    <action
        android:id="@+id/goSettingFragment"
        app:destination="@id/settingFragment" />
    <action
        android:id="@+id/goFeedbackFragment"
        app:destination="@id/feedbackFragment" />
    <action
        android:id="@+id/goLoginFragment"
        app:destination="@id/loginFragment" />
    <action
        android:id="@+id/goRegisterFragment"
        app:destination="@id/registerFragment" />

</navigation>